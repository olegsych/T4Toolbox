    <Metadata>
        <Identity Id="T4Toolbox.16" Version="|%CurrentProject%;GetPackageVersion|" Language="en-US" Publisher="Oleg Sych" />
        <DisplayName>T4 Toolbox for Visual Studio 2019</DisplayName>
