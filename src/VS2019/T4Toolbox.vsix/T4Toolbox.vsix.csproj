    <ProjectGuid>{965A6620-7CDD-405A-AFCD-5318F5F21A67}</ProjectGuid>
    <OutputType>Library</OutputType>
    <!-- On AppVeyor build agent, use dynamically generated package version. Locally, hardcode to 16.0.0.0 -->
