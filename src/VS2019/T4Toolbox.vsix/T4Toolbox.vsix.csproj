    <PackageVersion Condition=" '$(appveyor_build_version)' == '' ">16.0.0.0</PackageVersion>
    <!-- VSSDK properties -->
    <TargetVsixContainerName>T4Toolbox.$(PackageVersion).vsix</TargetVsixContainerName>
